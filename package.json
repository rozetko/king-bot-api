{
  "name": "king-bot-api",
  "version": "1.3.8",
  "description": "Advanced Bot for Travian Kingdoms",
  "main": "main-electron.js",
  "scripts": {
    "test": "tsc && webpack && npm run lint",
    "start": "node main.js",
    "dev": "ts-node dev_main.js",
    "build": "tsc && webpack",
    "server": "webpack-dev-server --mode development",
    "watch": "nodemon -e ts -w ./src -x npm run dev",
    "lint": "eslint --config .eslintrc.json --ext js --ext ts --ext jsx .",
    "lint:fix": "eslint --config .eslintrc.json --fix --ext js --ext ts --ext jsx .",
    "build:electron": "npm run build && webpack --config webpack.electron.config.js",
    "build:package": "npm run build && electron-packager --overwrite --electron-version=16.0.6 --arch=x64,ia32 --platform=darwin,linux,win32 --ignore=assets/ . --icon=public/images/icons/app.ico --ignore=^build-electron.sh$ --ignore=^docker.sh$ --ignore=^.gitignore$ --ignore=^.travis.yml$ --ignore=^.dockerignore$ --ignore=^.eslintignore$ --ignore=^.vs$ --ignore=^.vscode$ --ignore=^.github$ --ignore=^build-electron.sh$ --ignore=^Dockerfile$ --ignore=^rpi.Dockerfile$ --ignore=^README.md$ --ignore=^sample_main.js$ --ignore=^dev_main.js$",
    "build:package:bash": "bash build-electron.sh",
    "build:all": "npm run build:electron && npm run build:package",
    "build:docker:rpi": "docker build -t skunk1/king-bot-api:rpi -f rpi.Dockerfile .",
    "build:docker:latest": "docker build -t skunk1/king-bot-api:latest -f Dockerfile .",
    "build:docker": "npm run build:docker:latest && npm run build:docker:rpi",
    "push:docker:rpi": "npm run build:docker:rpi && docker push skunk1/king-bot-api:rpi",
    "push:docker:latest": "npm run build:docker:latest && docker push skunk1/king-bot-api:latest",
    "push:docker": "npm run build:docker && npm run push:docker:rpi && npm run push:docker:latest",
    "start:electron": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkeweloh/king-bot-api.git"
  },
  "keywords": [
    "bot",
    "advanced",
    "api"
  ],
  "author": "Felix Breuer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pkeweloh/king-bot-api/issues"
  },
  "homepage": "https://github.com/pkeweloh/king-bot-api#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "array-move": "^1.0.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.2.6",
    "datatables-bulma": "^1.0.1",
    "datatables.net": "^1.11.4",
    "deepmerge": "^2.2.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "https-proxy-agent": "^5.0.0",
    "jquery": "^3.6.0",
    "logform": "^2.1.0",
    "lowdb": "^1.0.0",
    "preact": "^8.5.3",
    "preact-router": "^2.6.1",
    "public-ip": "^3.2.0",
    "qs": "^6.10.2",
    "uniqid": "^5.1.0",
    "unistore": "^3.5.0",
    "winston": "^3.1.0",
    "winston-logzio": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@types/cheerio": "^0.22.30",
    "@types/express": "^4.17.2",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^10.17.60",
    "@types/qs": "^6.9.7",
    "@types/uniqid": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "acorn": "^6.4.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "electron": "^16.0.6",
    "electron-packager": "^15.4.0",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "ts-node": "^8.4.1",
    "typescript": "^4.5.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  }
}
